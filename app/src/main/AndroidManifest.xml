<?xml version="1.0" encoding="utf-8"?><manifest xmlns:android="http://schemas.android.com/apk/res/android"
package="dryhunch">
<uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
<uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
<uses-permission android:name="android.permission.INTERNET" />

<application
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:roundIcon="@mipmap/ic_launcher_round"
    android:supportsRtl="true"
    android:theme="@style/Theme.SmartCon">
    <activity
        android:name="com.example.dryhunch.student_home"
        android:exported="false" />
    <activity
        android:name="com.example.dryhunch.Onborading2"
        android:exported="false" />
    <activity
        android:name="com.example.dryhunch.MainActivity"
        android:exported="false" />

    <activity
        android:name="com.example.dryhunch.WeatherController"
        android:exported="false" />
    <activity
        android:name="com.example.dryhunch.prediction_page"
        android:exported="false" />

    <activity
        android:name="com.example.dryhunch.advice_page"
        android:exported="false" />

    <activity
        android:name="com.example.dryhunch.Onboarding1"
        android:exported="true">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>
</application>

</manifest>
